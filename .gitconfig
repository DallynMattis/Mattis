[core]
    editor = nvim
    autocrlf = input
[alias]
    s= status
    lg= log --all --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(auto)%d%C(reset)%n''  %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' 
    com= commit
